{
  "rules": {
    "semicolon": true,
    "no-conditional-assignment": true,
    "no-empty": true,
    "triple-equals": [true, "allow-null-check", "allow-undefined-check"],
    "quotemark": [true, "single", "avoid-escape"],
    "no-unused-expression": true,
    "no-unused-variable": [true, {"ignore-pattern": "^_"}]
  }
}
