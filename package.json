{
  "name": "com.inthetelling.story",
  "version": "0.0.0",
  "scripts": {
    "start": "rm -rf tmp/* && ./node_modules/.bin/webpack --env.dev local --progress --watch --colors",
    "prod": "rm -rf dist/* && ./node_modules/.bin/webpack --progress --colors --env.prod",
    "prod:local": "rm -rf tmp/* && ./node_modules/.bin/webpack --progress --colors --env.prod local",
    "prod:watch": "rm -rf tmp/* && ./node_modules/.bin/webpack --env.prod local --progress --colors --watch",
    "analyze": "rm -rf tmp/* && ./node_modules/.bin/webpack --env.prod analyze --progress --colors",
    "test": "./node_modules/.bin/karma start --browsers Chrome",
    "test:safari": "./node_modules/.bin/karma start --browsers Safari",
    "release": "cd utils/ts-releaser && npm run release"
  },
